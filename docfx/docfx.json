{
  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default"
    ],
    "globalMetadata": {
      "_appName": "PLATEAU PointCloud Generator",
      "_appTitle": "PLATEAU PointCloud Generator",
      "_enableSearch": false,
      "pdf": false,
      "_disableContribution": true
    }
  }
}